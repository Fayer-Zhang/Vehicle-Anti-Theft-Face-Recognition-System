<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".Settings">

    <include layout="@layout/toolbar" />

    <TextView
        android:id="@+id/settingstitle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="Settings"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="30dp"
        android:autoSizeMinTextSize="18dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:fontFamily="normal"
        app:layout_constraintTop_toTopOf="@+id/guideline6"
        app:layout_constraintBottom_toBottomOf="@+id/guideline7"
        app:layout_constraintStart_toStartOf="@id/guideline2"
        app:layout_constraintEnd_toEndOf="@id/guideline3"/>

    <TextView
        android:id="@+id/mdexplanation"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Add and remove drivers for this account"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="12dp"
        android:autoSizeMinTextSize="4dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:fontFamily="normal"
        app:layout_constraintTop_toTopOf="@+id/guideline8"
        app:layout_constraintBottom_toBottomOf="@+id/guideline11"
        app:layout_constraintStart_toStartOf="@id/guideline4"
        app:layout_constraintEnd_toEndOf="@id/guideline5" />

    <TextView
        android:id="@+id/enexplanation"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="manage face recognition"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="12dp"
        android:autoSizeMinTextSize="4dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:fontFamily="normal"
        app:layout_constraintTop_toTopOf="@+id/guideline14"
        app:layout_constraintBottom_toBottomOf="@+id/guideline15"
        app:layout_constraintStart_toStartOf="@id/guideline4"
        app:layout_constraintEnd_toEndOf="@id/guideline5" />

    <TextView
        android:id="@+id/pwexplanation"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Change account password"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="12dp"
        android:autoSizeMinTextSize="4dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:fontFamily="normal"
        app:layout_constraintTop_toTopOf="@+id/guideline18"
        app:layout_constraintBottom_toBottomOf="@+id/guideline19"
        app:layout_constraintStart_toStartOf="@id/guideline4"
        app:layout_constraintEnd_toEndOf="@id/guideline5" />

    <TextView
        android:id="@+id/spexplanation"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Report a problem"
        android:textColor="#ffffff"
        android:textSize="10dp"
        android:autoSizeMaxTextSize="12dp"
        android:autoSizeMinTextSize="4dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:fontFamily="normal"
        app:layout_constraintTop_toTopOf="@+id/guideline22"
        app:layout_constraintBottom_toBottomOf="@+id/guideline23"
        app:layout_constraintStart_toStartOf="@id/guideline4"
        app:layout_constraintEnd_toEndOf="@id/guideline5" />

    <TextView
        android:id="@+id/auexplanation"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Team and project description"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="12dp"
        android:autoSizeMinTextSize="4dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:fontFamily="normal"
        app:layout_constraintTop_toTopOf="@+id/guideline26"
        app:layout_constraintBottom_toBottomOf="@+id/guideline27"
        app:layout_constraintStart_toStartOf="@id/guideline4"
        app:layout_constraintEnd_toEndOf="@id/guideline5" />

    <Button
        android:id="@+id/managedrivers"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/outlinebutton"
        android:gravity="left|center_vertical"
        android:paddingLeft="5dp"
        android:paddingBottom="20dp"
        android:text="Manage drivers"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="20dp"
        android:autoSizeMinTextSize="12dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:layout_constraintTop_toTopOf="@+id/guideline9"
        app:layout_constraintBottom_toBottomOf="@+id/guideline10"
        app:layout_constraintEnd_toEndOf="@id/guideline1"
        app:layout_constraintStart_toStartOf="@id/guideline" />

    <Button
        android:id="@+id/enrollment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/outlinebutton"
        android:gravity="left|center_vertical"
        android:paddingLeft="5dp"
        android:paddingBottom="20dp"
        android:text="Enrollment"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="20dp"
        android:autoSizeMinTextSize="12dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:layout_constraintTop_toTopOf="@+id/guideline12"
        app:layout_constraintBottom_toBottomOf="@+id/guideline13"
        app:layout_constraintEnd_toEndOf="@id/guideline1"
        app:layout_constraintStart_toStartOf="@id/guideline" />

    <Button
        android:id="@+id/password"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/outlinebutton"
        android:gravity="left|center_vertical"
        android:paddingLeft="5dp"
        android:paddingBottom="20dp"
        android:text="Password"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="20dp"
        android:autoSizeMinTextSize="12dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        app:layout_constraintTop_toTopOf="@+id/guideline16"
        app:layout_constraintBottom_toBottomOf="@+id/guideline17"
        app:layout_constraintEnd_toEndOf="@id/guideline1"
        app:layout_constraintStart_toStartOf="@id/guideline" />

    <Button
        android:id="@+id/support"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/outlinebutton"
        android:gravity="left|center_vertical"
        android:paddingLeft="5dp"
        android:paddingBottom="20dp"
        android:text="Support"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:autoSizeMaxTextSize="20dp"
        android:autoSizeMinTextSize="12dp"
        android:autoSizeStepGranularity="2dp"
